public class MathOperationsDemo {

public static void main(String[] args) {
System.out.println("Welcome to the 'Math Operations Demo'!");

int numberA = 10;
int numberB = 10;


        int numberA = 10;
        int numberB = 10;
        int Addition = numberA + numberB;
        
        System.out.println("numberA + numberB = " + numberA + " + " + numberB + " = " + addition);

        long numberA = 10;
        long numberB = 10;
        long Difference = numberA - numberB;
        
        System.out.println("numberA - numberB = " + numberA + " - " + numberB + " = " + difference);

        short numberA = 90;
        short numberB = 30;
        short Multiplication = numberA * numberB;
        
        System.out.println("numberA * numberB = " + numberA + " * " + numberB + " = " + multiplication);

        byte numberA = 90;
        byte numberB = 30;
        byte  Division = (float)numberA / numberB;
        
        System.out.println("numberA / numberB = " + numberA + " / " + numberB + " = " + divivision);


 }

}
