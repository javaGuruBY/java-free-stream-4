public class ProductDemo {
    public static void main(String[] args) {
        Product firstProduct = new Product("Milk", 100, 25);
        Product secondProduct = new Product("Bread", 200, 25);
        ProductService productService = new ProductService();

        productService.printInformation(firstProduct);
        productService.actualPrice(firstProduct);
        System.out.println(" ");
        productService.printInformation(secondProduct);
        productService.actualPrice(secondProduct);
    }
}
