public class DogDemo {
    public static void main(String[] args){
        Dog firstDog = new Dog();
        Dog secondDog = new Dog();

        firstDog.age = 18; // возраст в месяцах
        firstDog.color = "black";
        firstDog.name = "Rax";

        System.out.println(firstDog.getName() + firstDog.getAge() + firstDog.getColor() + "He can:");
        firstDog.voice();
        firstDog.eat();
        firstDog.sleep();
        System.out.println("\n");

        secondDog.age = 26; // возраст в месяцах
        secondDog.color = "brown";
        secondDog.name = "Bob";

        System.out.println(secondDog.getName() + secondDog.getAge() + secondDog.getColor() + "He can:");
        secondDog.voice();
        secondDog.eat();
        secondDog.sleep();

        secondDog.name = "Tom"; // изменение свойства
        System.out.println("Sorry i was wrong!" + secondDog.getName());
        }
    }
