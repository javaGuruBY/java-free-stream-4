public class Encoder{
    private int code;
    private char symbol;

    public Encoder(int code, char symbol){
        this.code = code;
        this.symbol = symbol;
    }
    public Encoder(int code){
        this.code = code;
    }

    public Encoder(char symbol){
        this.symbol = symbol;
    }

    public void setSymbol(char symbol) {
        this.symbol = symbol;
    }

    public char getSymbol() {
        return symbol;
    }

    public void setCode(int code) {
        this.code = code;
    }

    public int getCode() {
        return code;
    }
}
